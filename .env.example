DEBUG=True
SECRET_KEY=dev-secret-key-change-me
ALLOWED_HOSTS=localhost,127.0.0.1

# Connect to Postgres service in docker-compose
# For docker-compose use: postgres://postgres:postgres@db:5432/investor_db
# For local development use: postgres://postgres:postgres@localhost:5432/investor_db
# Or leave empty to use SQLite: DATABASE_URL=
DATABASE_URL=postgres://postgres:postgres@db:5432/investor_db

# Financial defaults
VACANCY_RATE=0.05
MANAGEMENT_FEE_RATE=0.08
CAPEX_RESERVE_RATE=0.05

# Data source API keys
ATTOM_API_KEY=your_attom_api_key_here

# Redis configuration for caching, Channels, and Celery
# For docker-compose use: redis://redis:6379/0
# For local development use: redis://localhost:6379/0
REDIS_URL=redis://redis:6379/0

# Celery configuration
# Should match REDIS_URL
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Data source monitoring
ATTOM_MONTHLY_BUDGET=1000.00
ATTOM_COST_PER_CALL=0.01

